<view><view class="nav-top"><text>新鲜品尝</text></view><view class="float"><input class="uni-input" focus="{{true}}" placeholder="search"/><uni-icons class="iocns" vue-id="7c4ab00c-1" type="search" size="20" bind:__l="__l"></uni-icons></view><text class="nav-text2">每周上新</text><view class="week-nav"><text data-event-opts="{{[['tap',[['openWeekList',['$event']]]]]}}" class="week" bindtap="__e">综合</text><text data-event-opts="{{[['tap',[['salesSort',['$event']]]]]}}" class="week" bindtap="__e">销量</text><text class="week">价格</text></view><view class="icons1"><uni-icons class="icons2" vue-id="7c4ab00c-2" type="arrowdown" size="15" data-event-opts="{{[['^click',[['priceSortDown']]]]}}" bind:click="__e" bind:__l="__l"></uni-icons><uni-icons class="icons3" vue-id="7c4ab00c-3" type="arrowup" size="15" data-event-opts="{{[['^click',[['priceSortUp']]]]}}" bind:click="__e" bind:__l="__l"></uni-icons></view><view class="content"><block wx:for="{{weekList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="content-list"><view style="width:30%;"><image class="image" src="{{url+item.photo}}"></image></view><view style="width:70%;"><text class="title">{{item.product_name}}</text><view class="volume"><text>{{item.volume}}</text></view><view class="content_right_3"><text class="price">{{"￥"+item.price}}</text><text style="color:white;background-color:orange;">明日达</text><uni-icons vue-id="{{'7c4ab00c-4-'+index}}" type="plus" size="20" color="red" data-event-opts="{{[['^click',[['addCart',['$0','$1','$2'],[[['weekList','',index,'photo']],[['weekList','',index,'product_name']],[['weekList','',index,'price']]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-icons></view></view></view></block></view></view>